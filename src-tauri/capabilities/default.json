{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "binaries/werkday-server",
          "sidecar": true
        }
      ]
    },
    "shell:allow-spawn",
    "shell:allow-kill"
  ]
}
